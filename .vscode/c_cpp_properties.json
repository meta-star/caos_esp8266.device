{
  "configurations": [
    {
      "name": "esp-dev",
      "includePath": [
        "${workspaceRoot}",
        "${workspaceRoot}/build",
        "${workspaceRoot}/build/include",
        //"${workspaceRoot}/components",
        "${env:IDF_PATH}/components/**"
      ],
      "intelliSenseMode": "clang-x64",
      "browse": {
        "path": [
          "${workspaceRoot}",
          "${workspaceRoot}/build",
          "${workspaceRoot}/build/include",
          //"${workspaceRoot}/components",
          "${env:IDF_PATH}/components/**"
        ],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": "${workspaceRoot}/.vscode/browse.vc.db"
      },
      "cStandard": "c11",
      "cppStandard": "c++17"
    }
  ],
  "version": 4
}
